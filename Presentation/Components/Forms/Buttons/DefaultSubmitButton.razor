<MudButton Variant="Variant.Filled" StartIcon="@Icons.Material.Filled.Check" IconColor="Color.Success"
           Class="btn-standard rounded" OnClick="@Submit">
    Ok
</MudButton>

@code {

    [CascadingParameter]
    public IBaseForm? Form { get; set; }

    private void Submit() => Form?.SubmitAsync();
}
